    "\n",
    "def depth(top , x):\n",
    "  \n",
    "  def dfs(node , x , cnt):\n",
    "    cnt+=1\n",
    "    if node is  None:\n",
    "      return \n",
    "    if node == x:\n",
    "      print(cnt)\n",
    "      exit()\n",
    "    \n",
    "    dfs(node.left , x , cnt)\n",
    "    dfs(node.right , x , cnt)\n",
    "  \n",
    "  dfs(top , x , -1)\n",
    "  return -1"
